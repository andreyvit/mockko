      Mockko: iPhone Mockups | iPad Mockups | Android Mockups
    %script
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-17047204-1']);
      _gaq.push(['_setDomainName', 'none']);
      _gaq.push(['_setAllowLinker', true]);
      _gaq.push(['_trackPageview']);
