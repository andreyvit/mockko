%html
  %head
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    %link(rel="stylesheet" href="./static/home.css" type="text/css" media="screen" title="no title" charset="utf-8")
    %script(src="./static/lib/jquery-1.4.2.js" type="text/javascript" charset="utf-8")
    %script(src="./static/lib/underscore.js" type="text/javascript" charset="utf-8")
